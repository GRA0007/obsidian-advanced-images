{
  "gitSiteUrl": "https://github.com/GRA0007/obsidian-advanced-images/",
  "changeTags": {
    "feat": "New Features",
    "fix": "Bug Fixes",
    "perf": "Performance Improvements"
  },
  "packages": {
    "obsidian-advanced-images": {
      "version": true,
      "path": ".",
      "releaseTag": "${ pkgFile.version }",
      "getPublishedVersion": "curl -s https://api.github.com/repos/GRA0007/obsidian-advanced-images/releases/latest | jq -r '.tag_name'",
      "publish": "echo publish"
    }
  }
}
